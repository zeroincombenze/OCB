"\n"
"\n"
".. code-block:: python\n"
"\n"
"    parser = [\n"
"        'name',\n"
"        'number',\n"
"        'create_date',\n"
"        ('partner_id', ['id', 'display_name', 'ref'])\n"
"        ('line_id', ['id', ('product_id', ['name']), 'price_unit'])\n"
"    ]\n"
"\n"
